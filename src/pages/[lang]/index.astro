---
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return [
    { params: { lang: "en" } },
    { params: { lang: "es" } },
    { params: { lang: "pt" } },
  ];
}

const { lang } = Astro.params;
---

<Layout
  title="RPCIDE"
  description="RPCIDE is a platform for developers to create and share RPC interfaces."
>
  <h1>Welcome to the Language Page {lang}</h1>
</Layout>
